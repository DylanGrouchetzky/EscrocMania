{% for picture in field.value %}
    {% set html_id = 'ea-lightbox-' ~ field.uniqueId ~ '-' ~ loop.index %}
    <a href="#" class="ea-lightbox-thumbnail" data-ea-lightbox-content-selector="#{{ html_id }}">
        <img src="{{ asset('/img/products/' ~picture.file) }}" class="img-fluid">
    </a>

    <div id="{{ html_id }}" class="ea-lightbox">
        <img src="{{ asset('/img/products/' ~picture.file) }}">
    </div>
{% endfor %}